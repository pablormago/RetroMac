IF NOT "$WBDIR" EQ ""
   RequestFile $WBDIR/#?.info >ENV:WBINFO
ELSE
   RequestFile #?.info >ENV:WBINFO
ENDIF
IF NOT "$WBINFO" EQ "" >NIL:
   List $WBINFO QUICK NOHEAD LFORMAT %E TO ENV:WBEXT
   IF "$WBEXT" EQ "info"
      List $WBINFO QUICK NOHEAD FILES LFORMAT %F%M TO ENV:WBRUN
      List $WBRUN#? QUICK NOHEAD DIRS LFORMAT %F%M TO ENV:WBDIR
      IF NOT "$WBDIR" EQ ""
         Execute S:WBSelect
      ELSE
         WBRun "$WBRUN" SYNC >NIL:
      ENDIF
   ELSE
      List $WBINFO QUICK NOHEAD LFORMAT %F%N TO ENV:WBRUN
      $WBRUN >NIL:
   ENDIF
   UNSETENV WBEXT
ENDIF
UNSETENV WBINFO
